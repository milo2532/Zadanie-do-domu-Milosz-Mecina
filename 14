def count_letters_and_digits(string):
    num_letters = 0
    num_digits = 0
    for char in string:
        if char.isalpha():
            num_letters += 1
        elif char.isdigit():
            num_digits += 1
    return num_letters, num_digits

def main():
    string = input("Enter a string: ")
    letters, digits = count_letters_and_digits(string)
    print("Letters:", letters)
    print("Digits:", digits)

if __name__ == "__main__":
    main()
