def count_same_first_last_substrings(s):
    count = 0
    n = len(s)
    for i in range(n):
        for j in range(i, n):
            if s[i] == s[j]:
                count += 1
    return count

# Example usage
input_string = "ababa"
result = count_same_first_last_substrings(input_string)
print(f"Number of substrings with the same first and last characters in '{input_string}': {result}")
