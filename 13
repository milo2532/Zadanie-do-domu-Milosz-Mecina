def binary_divisible_by_5(binary_numbers):
    divisible_by_5 = []
    for num in binary_numbers:
        decimal_num = int(num, 2)
        if decimal_num % 5 == 0:
            divisible_by_5.append(num)
    return divisible_by_5

def main():
    input_string = input("Enter comma separated 4 digit binary numbers: ")
    binary_numbers = input_string.split(',')
    divisible_by_5 = binary_divisible_by_5(binary_numbers)
    if divisible_by_5:
        print("Numbers divisible by 5:", ', '.join(divisible_by_5))
    else:
        print("No numbers divisible by 5 found.")

if __name__ == "__main__":
    main()
