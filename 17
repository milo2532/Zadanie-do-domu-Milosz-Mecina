def print_pattern_A():
    for row in range(7):
        for col in range(5):
            if (row == 0 and col % 2 != 0) or (row > 0 and (col == 0 or col == 4)) or (row == 3 and col != 0 and col != 4):
                print("*", end="")
            else:
                print(" ", end="")
        print()

print("Alphabet Pattern 'A':")
print_pattern_A()
