dict1 = {'a': 1, 'b': 2}
dict2 = {'c': 3, 'd': 4}
dict3 = {'e': 5, 'f': 6}

concatenated_dict = dict1.copy()  # Make a copy of the first dictionary
concatenated_dict.update(dict2)   # Update it with the second dictionary
concatenated_dict.update(dict3)   # Update it with the third dictionary

print("Concatenated dictionary using update method:")
print(concatenated_dict)
